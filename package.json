{
  "name": "shear",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "node --loader ts-node/esm src/server/server.ts",
    "dev-external": "vite --host 0.0.0.0 --port 5173",
    "serve-external": "node --no-warnings --loader ts-node/esm src/server/server.ts --host 0.0.0.0 --port 3000",
    "all": "concurrently \"npm run serve\" \"npm run dev\"",
    "build": "tsc && vite build",
    "build-sketchy": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-server": "tsc -p src/server/tsconfig-server.json",
    "run-server-ec2": "node serverDist/server.js",
    "run-server-local" : "node dist/serverDist/server.js",
    "deploy": "aws cloudformation create-stack --region us-east-2 --stack-name TEST50 --template-body file://./src/scripts/cloudFormationDeploy.yaml --capabilities CAPABILITY_IAM",
    "packer": "packer build src/scripts/packer/aws-ubuntu.pkr.hcl"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.465.0",
    "@aws-sdk/client-dynamodb": "^3.495.0",
    "@aws-sdk/client-lambda": "^3.465.0",
    "@aws-sdk/util-dynamodb": "^3.495.0",
    "@aws-sdk/util-utf8-node": "^3.259.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/material": "^5.14.19",
    "@reduxjs/toolkit": "^1.9.7",
    "adm-zip": "^0.5.10",
    "aws-sdk": "^2.1530.0",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "marshall": "^1.3.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-d3-library": "^1.1.8",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "recharts": "^2.10.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "ts-node": "^10.9.1",
    "typescript": "5.0",
    "vite": "^5.0.0"
  }
}
